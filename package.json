{
  "name": "reportsuites.com",
  "version": "0.7.5",
  "description": "A website which creates excel files with all Adobe reportsuite configuration options",
  "main": "client-handler.js",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "browser-request": "^0.3.3",
    "browserify": "^14.4.0",
    "file-saver": "^1.3.3",
    "handlebars": "^4.0.10",
    "hbsfy": "^2.8.1",
    "jquery": "^3.2.1",
    "jszip": "^3.1.3",
    "keen-tracking": "^1.4.0",
    "lodash": "^4.17.4",
    "pbkdf2": "^3.0.14",
    "wsse": "^2.4.0",
    "xmlbuilder": "^9.0.4"
  },
  "devDependencies": {
    "browserify-handlebars": "^1.0.0",
    "jest": "^22.0.6",
    "minifyify": "^7.3.5",
    "uglifyify": "^4.0.5"
  },
  "scripts": {
    "coverage": "jest --coverage",
    "test": "jest",
	"build": "browserify client-handler.js -o adobe-export.js",
    "watch": "watchify -t hbsfy -g [uglifyify -x .js --mangle --compress [--unused --join_vars --properties --dead_code] ] client-handler.js -o adobe-export.js -v",
    "bundle": "browserify -t hbsfy -g [uglifyify -x .js --mangle --compress [--unused --join_vars --properties --dead_code] ] client-handler.js -o adobe-export.js -v",
	"build-essentials": "browserify -t hbsfy -g [uglifyify -x .js --mangle --compress [--unused --join_vars --properties --dead_code] ] essentials-setup.js -o assets/js/essentials.js -v",
    "mocks": "handlebars progress-display.handlebars -f __mocks__/progress-display.js && handlebars select_rsid.handlebars -f __mocks__/select_rsid.js"
  },
  "main":"client-handler.js",
  "keywords": [
    "adobe",
    "analytics",
    "excel"
  ],
  "author": {
    "name": "Gene Jones",
    "email": "iam@genejon.es",
    "url": "genejon.es"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/generjones/reportsuites.com.git"
  },
  "license": "Apache-2.0",
  "jest": {
    "modulePathIgnorePatterns": [
      ".handlebars"
    ],
    "moduleNameMapper": {
      "progress-display.handlebars$": "<rootDir>/__mocks__/progress-display.js",
      "select_rsid.handlebars$": "<rootDir>/__mocks__/select_rsid.js"
    },
	"setupFiles": ["<rootDir>/__mocks__/jquery.js"]
  },
  "browserify" : {
	"entry": "client-handler.js",
	"transform": [
		"browserify-shim",
		"hbsfy"
	]
  },
  "browserify-shim": {
	"jQuery": "global:jQuery"
  }
}
